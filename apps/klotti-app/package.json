{
  "name": "@klotti/klotti",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "local": "NODE_ENV=local expo start --dev-client",
    "dev": "NODE_ENV=development expo start --dev-client",
    "web": "NODE_ENV=development expo start --web",
    "production": "NODE_ENV=production expo start --dev-client",
    "ios": "NODE_ENV=development expo run:ios",
    "ios:prod": "NODE_ENV=production expo run:ios",
    "android": "NODE_ENV=development expo run:android",
    "android:prod": "NODE_ENV=production expo run:android",
    "deploy:dev:ota": "expo prebuild && NODE_ENV=development eas update --channel development",
    "deploy:prod:ota": "expo prebuild && NODE_ENV=production eas update --channel production",
    "deploy:ios:dev:store": "expo prebuild && NODE_ENV=development eas build --profile development --platform ios --auto-submit",
    "deploy:android:dev:store": "expo prebuild && NODE_ENV=development eas build --profile development --platform android --auto-submit",
    "deploy:dev:store": "expo prebuild && NODE_ENV=development eas build --profile development --platform all --auto-submit",
    "deploy:ios:prod:store": "expo prebuild && NODE_ENV=production eas build --profile production --platform ios --auto-submit",
    "deploy:android:prod:store": "expo prebuild && NODE_ENV=production eas build --profile production --platform android --auto-submit",
    "deploy:prod:store": "expo prebuild && NODE_ENV=production eas build --profile production ---platform all --auto-submit",
    "deploy:web:dev": "NODE_ENV=development npx expo export:web && cd web-dev && npx vercel",
    "gql:generate:local": "NODE_ENV=local graphql-codegen",
    "gql:generate:development": "NODE_ENV=development graphql-codegen",
    "gql:generate:production": "NODE_ENV=production graphql-codegen",
    "test": "jest --passWithNoTests",
    "clean": "rm -rf node_modules && rm -rf ios && rm -rf android && rm -rf dist",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@apollo/client": "^3.8.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/runtime": "^7.18.9",
    "@expo/vector-icons": "^13.0.0",
    "@faker-js/faker": "^8.0.2",
    "@gorhom/bottom-sheet": "4.5.1",
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/typescript": "^3.0.3",
    "@graphql-codegen/typescript-operations": "^3.0.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@hookform/resolvers": "^2.9.10",
    "@klotti/ui": "*",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-firebase/app": "^18.5.0",
    "@react-native-firebase/auth": "^18.5.0",
    "@react-native-google-signin/google-signin": "^10.0.1",
