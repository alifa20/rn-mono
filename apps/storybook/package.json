{
  "name": "storybook",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher",
    "dev": "sb-rn-get-stories && cross-env expo start --dev-client --port 1111",
    "web:dev": "expo prebuild && sb-rn-get-stories && cross-env expo --web --port 1111",
    "ios": "expo prebuild && sb-rn-get-stories && cross-env expo run:ios --port 1111",
    "android": "expo prebuild && sb-rn-get-stories && cross-env expo run:android --port 1111",
    "clean": "rm -rf node_modules && rm -rf .expo && rm -rf ios && rm -rf android"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo/vector-icons": "^13.0.0",
    "@gorhom/bottom-sheet": "4.5.1",
    "@klotti/ui": "*",
    "@react-native-picker/picker": "2.4.10",
    "@shopify/flash-list": "^1.5.0",
    "@shopify/restyle": "^2.4.2",
    "expo": "~49.0.3",
    "expo-constants": "~14.4.2",
    "expo-image": "~1.3.2",
    "expo-linear-gradient": "~12.3.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
